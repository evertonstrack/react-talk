{
  "name": "react-talk",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-scripts": "1.1.5"
  },
  "scripts": {
    "start": "sass --update src/assets/styles/scss:src/assets/styles/css && react-scripts start",
    "sass:build": "sass --update src/assets/styles/scss:src/assets/styles/css --style compressed",
    "build": "npm run sass:build && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "watch": "sass --watch src/assets/styles/scss:src/assets/styles/css"
  },
  "devDependencies": {
    "react-router-dom": "^4.3.1",
    "sass": "^1.13.1"
  }
}
